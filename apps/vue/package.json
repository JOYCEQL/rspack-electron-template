{
	"name": "vue",
	"private": true,
	"version": "1.0.0",
	"main": "electron/main.js",
	"scripts": {
		"dev": "rsbuild dev",
		"build": "rsbuild build",
		"preview": "rsbuild preview",
		"electron": "wait-on http://localhost:3001 && electron .",
		"electron-dev": "concurrently \"pnpm dev\" \"pnpm electron\"",
		"electron-build": "pnpm build && electron-builder"
	},
	"dependencies": {
		"vue": "^3.5.12",
		"electron-is-dev": "^2.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.3",
		"@rsbuild/core": "^1.0.13",
		"@rsbuild/plugin-vue": "^1.0.1",
		"typescript": "^5.6.3",
		"electron": "^24.3.0"
	},
	"build": {
		"appId": "com.example.rspack-electron-app",
		"files": ["./dist/**/*", "./electron/**/*", "./package.json"],
		"directories": {
			"output": "./release"
		}
	}
}
